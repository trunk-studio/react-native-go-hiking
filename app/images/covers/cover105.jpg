    var _tabs = function(o){
        // init
        o.init = function(){
            var self = this;
            self.$el = $(self.el);
            self.$items = self.$el.find('ul > li > a');

            self.tabContents = _tabContents({
                el: '#event-content'
            });

            self.$items.mouseover(function(e){
                var $this = $(this),
                    current = parseInt($this.text());
                self.$items.removeClass('in');
                $this.addClass('in');
                self.tabContents.display(current);
            });
        }

        return o.init();
    }

    /**
     * prototype of tab content
     * @params (object) o, default: el
     * @return (object) tabContent
     */
    var _tabContents = function(o){
        // init
        o.init = function(){
            var self = this;
            self.$el = $(self.el);
            self.$contents = self.$el.find('.ch_content_body');
            return self;
        }

        o.display = function(key){
            key--;
            this.$contents.each(function(idx, obj){
                if (idx!=key)
                    $(obj).addClass('hide');
                else
                    $(obj).removeClass('hide')
            });
        }

        return o.init();
    }

    $(function(){
        var tab = _tabs({
            el: '#event-tab'
        });
    });
</script>
